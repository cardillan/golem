#set target = 8W;
#set symbolic-labels = true;

module clear;

const minX = 200;
const maxX = 400;

const minY = 0;
const maxY = 150;

begin
    setrate(1000);
end;

export void clearArea1()
    for var x in minX ... maxX do
        for var y in minY ... maxY do
            var building = getblock(:building, x, y);
            if building.@ammoCapacity > 0 then
                setblock(:block, @air, x, y, 0, 0);
            end;
        end;
    end;
end;
